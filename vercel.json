{
    "version": 2,
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/src/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "api/index.py"
        }
    ],
    "functions": {
        "api/index.py": {
            "maxDuration": 30
        }
    },
    "env": {
        "PYTHONPATH": "/var/task"
    }
}